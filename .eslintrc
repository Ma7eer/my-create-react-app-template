{
    "parser": "babel-eslint",
    "plugins": ["react", "jest"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "eslint-config-prettier"
    ],
    "rules": {
        "no-console": "warn"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest/globals": true
    }
}